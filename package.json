{
  "name": "tsdx",
  "version": "0.0.0",
  "author": "Takeshi Iwana <iwatakeshi@gmail.com>",
  "description": "Zero-config TypeScript package development",
  "license": "MIT",
  "type": "commonjs",
  "homepage": "https://github.com/prismify-co/tsdx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prismify-co/tsdx.git"
  },
  "keywords": [
    "react",
    "typescript",
    "bundle",
    "rollup"
  ],
  "bugs": {
    "url": "https://github.com/prismify-co/tsdx/issues"
  },
  "bin": {
    "tsdx": "./dist/src/index.js"
  },
  "scripts": {
    "prepare": "tsc -p tsconfig.json",
    "build": "tsc -p tsconfig.json",
    "lint": "yarn build && yarn lint:post-build",
    "lint:post-build": "node dist/src/index.js lint ./ --ignore-pattern 'test/e2e/fixtures/**/*'",
    "test": "yarn build && yarn test:post-build",
    "test:post-build": "node dist/src/index.js test",
    "start": "tsc -p tsconfig.json --watch",
    "release": "np",
    "deduplicate": "yarn-deduplicate -s fewer yarn.lock",
    "deduplicate:check": "yarn-deduplicate -s fewer yarn.lock --list --fail"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/helper-module-imports": "^7.13.12",
    "@babel/parser": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "@babel/traverse": "^7.13.15",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "ansi-escapes": "^4.3.1",
    "asyncro": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-polyfill-regenerator": "^0.2.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.0",
    "comment-json": "^4.1.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "jest-watch-typeahead": "^0.6.1",
    "jpjs": "^1.2.1",
    "lodash.merge": "^4.6.2",
    "ora": "^5.3.0",
    "pascal-case": "^3.1.2",
    "prettier": "^2.2.1",
    "progress-estimator": "^0.3.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.45.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semver": "^7.3.4",
    "shelljs": "^0.8.4",
    "tiny-glob": "^0.2.8",
    "tslib": "^2.2.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.10",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/sade": "^1.7.2",
    "@types/semver": "^7.3.4",
    "@types/shelljs": "^0.8.8",
    "@types/styled-components": "^5.1.9",
    "@types/yargs": "^16.0.1",
    "autoprefixer": "^10.2.4",
    "babel-plugin-replace-identifiers": "^0.1.1",
    "cssnano": "^5.0.1",
    "doctoc": "^2.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "np": "^7.4.0",
    "postcss": "^8.2.7",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "styled-components": "^5.2.1",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "yarn-deduplicate": "^3.1.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
